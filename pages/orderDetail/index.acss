page {
  padding-bottom: 147rpx;
  background-color: #f0f0f0;
}

page .logistics-dynamics {
  background: #FA4E62;
  height: 160rpx;
  padding-left: 30rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

page .logistics-dynamics .top {
  display: flex;
  align-items: center;
}

page .logistics-dynamics .top image {
  width: 45rpx;
  height: 45rpx;
}

page .logistics-dynamics .top text {
  margin-left: 16rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #FFFFFF;
}

page .logistics-dynamics .bottom {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  margin-top: 24rpx;
  position: relative;
}

page .logistics-dynamics .bottom::after {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 28rpx;
  content: ' ';
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 12rpx 20rpx;
  width: 12rpx;
  height: 20rpx;
}

page .address-box {
  margin-top: 28rpx;
  background-color: #FFF;
  height: 160rpx;
  display: flex;
  align-items: flex-start;
  padding-left: 27rpx;
  padding-top: 34rpx;
  box-sizing: border-box;
}

page .address-box image {
  width: 27rpx;
  height: 33rpx;
}

page .address-box .right {
  padding-left: 23rpx;
  padding-top: 3rpx;
  box-sizing: border-box;
}

page .address-box .right .user-info {
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #212121;
}

page .address-box .right .user-info text:last-child {
  margin-left: 40rpx;
}

page .address-box .right .address-name {
  margin-top: 31rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(33, 33, 33, 0.5);
}

page .shop-box {
  background-color: #FFF;
  margin-top: 28rpx;
  padding: 16rpx 30rpx 23rpx;
  box-sizing: border-box;
}

page .shop-box .shop-info {
  display: flex;
  align-items: center;
}

page .shop-box .shop-info image {
  width: 57rpx;
  height: 57rpx;
}

page .shop-box .shop-info .shop-name {
  margin-left: 26rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #212121;
  padding-right: 26rpx;
  box-sizing: border-box;
  position: relative;
}

page .shop-box .shop-info .shop-name::after {
  position: absolute;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
  content: ' ';
  width: 12rpx;
  height: 23rpx;
  background-image: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAkAAAAOCAQAAABXnf4jAAAAAXNSR0IArs4c6QAAAHRJREFUGBkFwS0SQQEABsBNfucNV6EQKBQSifxuZNyFQqEQuApjGIPy2YWVBQBQiq8ZAFRtxccEAOr24m0EAE1H8TQAgMJZPPQAoO0qbjoAtFzEXRegcBIPfYCGg3gZAtTsxNsYoGIjPqYAlOJnDgCsLQHgDzkTJPRjJR71AAAAAElFTkSuQmCC");
  background-repeat: no-repeat;
  background-size: 12rpx 23rpx;
}

page .shop-box .product {
  margin-top: 39rpx;
  display: flex;
}

page .shop-box .product image {
  width: 160rpx;
  height: 160rpx;
}

page .shop-box .product .right-info {
  flex: 1;
  padding-left: 20rpx;
  box-sizing: border-box;
}

page .shop-box .product .right-info .product-name {
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #212121;
  line-height: 42rpx;
}

page .shop-box .product .right-info .product-specification {
  margin-top: 16rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #666666;
}

page .shop-box .product .right-info .price {
  margin-top: 15rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #EB5A63;
}

page .shop-box .product .right-info .credit {
  margin-top: 15rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

page .shop-box .product .right-info .credit text:first-child {
  display: inline-block;
  width: 181rpx;
  height: 43rpx;
  line-height: 43rpx;
  text-align: center;
  background: #FFFFFF;
  border: 1rpx solid #54B67D;
  border-radius: 4rpx;
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #54B67D;
}

page .shop-box .product .right-info .credit text:last-child {
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #212121;
  padding: 11rpx 21rpx;
  box-sizing: border-box;
  background: #E0E0E0;
  border-radius: 21rpx;
}

page .lease-term-box {
  background-color: #FFF;
  margin-top: 28rpx;
  padding: 0 28rpx;
  box-sizing: border-box;
}

page .lease-term-box .top-box .row-item {
  height: 84rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #212121;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

page .lease-term-box .top-box .black {
  color: #212121;
}

page .lease-term-box .top-box .gray {
  font-weight: 500;
  color: rgba(33, 33, 33, 0.6);
}

page .lease-term-box .bottom-box {
  height: 144rpx;
  position: relative;
  display: flex;
  justify-content: center;
  flex-direction: column;
}

page .lease-term-box .bottom-box::before {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  content: ' ';
  height: 1rpx;
  background: #E0E0E0;
}

page .lease-term-box .bottom-box view {
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #212121;
}

page .lease-term-box .bottom-box view:first-child text:last-child {
  color: #EB5A63;
}

page .lease-term-box .bottom-box view:last-child {
  margin-top: 27rpx;
  font-weight: 500;
  color: rgba(33, 33, 33, 0.6);
}

page .order-info {
  background-color: #FFF;
  margin-top: 28rpx;
  padding: 0 39rpx 0 36rpx;
  box-sizing: border-box;
}

page .order-info .orider-item {
  height: 87rpx;
  display: flex;
  align-items: center;
  color: rgba(33, 33, 33, 0.6);
  justify-content: space-between;
}

page .order-info .orider-item .title {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
}

page .order-info .orider-item .right {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
}

page .order-info .orider-item .right text {
  display: inline-block;
  margin-left: 14rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  height: 39rpx;
  line-height: 39rpx;
  width: 101rpx;
  text-align: center;
  background: #FFFFFF;
  border: 1rpx solid #E0E0E0;
  border-radius: 19rpx;
}

page .protocol {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #3390FF;
  text-align: center;
  margin-top: 32rpx;
}

page .operting {
  position: fixed;
  bottom: 0;
  background-color: #FFF;
  left: 0;
  width: 100%;
  padding: 0 40rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  height: 86rpx;
}

page .operting text {
  padding: 15rpx 25rpx;
  box-sizing: border-box;
  background: #FFFFFF;
  border: 1rpx solid #E0E0E0;
  border-radius: 30rpx;
  margin-left: 30rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #212121;
}
